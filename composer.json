{
    "name": "kwhat/http-middleware",
    "description": "",
    "homepage": "https://github.com/kwhat/http-middleware",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name":"Alex Barker",
            "email": "alex@1stleg.com",
            "homepage":"http://github.com/kwhat"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "psr/http-message": "^1.0",
        "psr/http-server-handler": "^1.0",
        "psr/http-server-middleware": "^1.0"
    },
    "require-dev": {
        "phan/phan": "^2.2",
        "phpunit/phpunit": "^8",
        "squizlabs/php_codesniffer": "3.*"
    },
    "archive": {
        "exclude": ["build/", "composer.lock"]
    },
    "autoload": {
        "psr-4": {
            "What4\\Http\\Middleware\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "What4\\Http\\Middleware\\Test\\": "tests"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "secure-http": false,
        "sort-packages": true
    },
    "scripts": {
        "lint": [
            "phpcs --standard=PSR1,PSR2 src/ tests/",
            "phan"
        ],
        "test": [
            "phpunit --coverage-html build/coverage tests/"
        ]
    }
}
